		<script>
			var vm = new Vue({
				el: "#app",
				data: {
					msg: [],
					order:[]
				},

				created() {
					url1 = 'http://localhost:8080/CloudPlatform/FirstPageServlet';
					console.log("连接成功");
					promiseGet(url1)
						.then(res => {
							console.log(res);
							vm.msg = res;
						}).catch(err => {
							console.log(err);
						})
						
					url2 = 'http://localhost:8080/CloudPlatform/FirstPageShowProductServlet';
					console.log("连接成功");
					promiseGet(url2)
						.then(res => {
//							console.log(res[3].factoryId);
							vm.order = res;					
						}).catch(err => {
							console.log(err);
						})
				}
			})
		</script>
