package com.summer.pojo;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
@AllArgsConstructor
public class Permit {

	private Integer id; // 权限ID
	private Integer flag; // 有效标识 0:有效 1:无效
	private Integer status; // 权限状态 0:正常 1:停用
	private Integer parentId; // 权限父ID
	private String permitName; // 权限名称
	private String permitDesc; // 权限描述
	private String permitPath; // 校验地址

}
